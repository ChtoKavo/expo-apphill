{
  "expo": {
    "owner": "popka_artema123",
    "name": "ChillOut",
    "slug": "avchpyvapyvapyvap",
    "scheme": "chillout",
    "notification": {
      "icon": "./assets/ChillOut.jpg",
      "color": "#667eea",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} новых уведомлений",
      "iosDisplayInForeground": true
    },
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ChillOut.jpg",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/ChillOut.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ChillOut.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.chillout.app",
      "versionCode": 3,
      "usesCleartextTraffic": true,
      "networkSecurityConfig": "network_security_config",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "VIBRATE",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "POST_NOTIFICATIONS",
        "FOREGROUND_SERVICE"
      ]
    },
    "plugins": [
      "expo-dev-client",
      "expo-task-manager",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "networkSecurityConfig": "network_security_config"
          }
        }
      ]
    ],
    "web": {
      "favicon": "./assets/ChillOut.jpg"
    },
    "extra": {
      "eas": {
        "projectId": "f5af9aaa-6f13-457c-a15f-928173d008da"
      }
    }
  }
}
